module latch1_vlg_vec_tst();
// constants                                           
// general purpose registers
reg [7:0] A;
reg Clock;
reg Resetn;
// wires                                               
wire [7:0] Q;

wire sampler;                             

// assign statements (if any)                          
latch1 i1 (
// port map - connection between master ports and signals/registers   
	.A(A),
	.Clock(Clock),
	.Q(Q),
	.Resetn(Resetn)
);
// A[ 7 ]
initial
begin
	A[7] = 1'b0;
end 
// A[ 6 ]
initial
begin
	A[6] = 1'b0;
end 
// A[ 5 ]
initial
begin
	A[5] = 1'b0;
end 
// A[ 4 ]
initial
begin
	A[4] = 1'b0;
end 
// A[ 3 ]
initial
begin
	A[3] = 1'b0;
	A[3] = #80000 1'b1;
end 
// A[ 2 ]
initial
begin
	A[2] = 1'b0;
	A[2] = #40000 1'b1;
	A[2] = #40000 1'b0;
end 
// A[ 1 ]
initial
begin
	repeat(2)
	begin
		A[1] = 1'b0;
		A[1] = #20000 1'b1;
		# 20000;
	end
	A[1] = 1'b0;
end 
// A[ 0 ]
always
begin
	A[0] = 1'b0;
	A[0] = #10000 1'b1;
	#10000;
end 

// Clock
always
begin
	Clock = 1'b0;
	Clock = #10000 1'b1;
	#10000;
end 

// Resetn
initial
begin
	Resetn = 1'b0;
	Resetn = #10000 1'b1;
end 

latch1_vlg_sample_tst tb_sample (
	.A(A),
	.Clock(Clock),
	.Resetn(Resetn),
	.sampler_tx(sampler)
);

latch1_vlg_check_tst tb_out(
	.Q(Q),
	.sampler_rx(sampler)
);
endmodule

