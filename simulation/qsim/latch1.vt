`timescale 1 ps/ 1 ps
module latch1_vlg_sample_tst(
	A,
	Clock,
	Resetn,
	sampler_tx
);
input [7:0] A;
input  Clock;
input  Resetn;
output sampler_tx;

reg sample;
time current_time;
always @(A or Clock or Resetn)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

module latch1_vlg_check_tst (
